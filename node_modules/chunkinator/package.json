{
  "name": "chunkinator",
  "version": "2.0.5",
  "description": "Upload large files from the browser with ease",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "atlaskit:src": "src/index.ts",
  "scripts": {
    "test": "jest --config jest.config.json",
    "test:watch": "jest --watch --config jest.config.json",
    "start": "webpack -w",
    "dev": "webpack-dev-server",
    "test:ci": "jest --runInBand --coverage --config jest.config.json",
    "build": "rm -rf dist && NODE_ENV=production tsc -p ./tsconfig.prod.json",
    "release": "npm version patch && git push --tags && git push && npm publish",
    "prepublishOnly": "yarn test:ci && yarn build",
    "perf:build": "yarn build && webpack --config webpack.perf.config.js",
    "perf:stats": "webpack --config webpack.perf.config.js --json > perf/dist/stats.json",
    "perf:analyze": "webpack-bundle-analyzer perf/dist/stats.json",
    "perf": "mkdir -p perf/dist && yarn perf:build && yarn perf:stats && yarn perf:analyze"
  },
  "repository": "git@bitbucket.org:hzarcogarcia/chunkinator.git",
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "awesome-typescript-loader": "^3.4.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.2.2",
    "typescript": "~2.6.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "rxjs": "^5.5.6",
    "rxjs-async-map": "^0.1.2",
    "tslib": "^1.8.0"
  },
  "files": [
    "dist"
  ]
}
